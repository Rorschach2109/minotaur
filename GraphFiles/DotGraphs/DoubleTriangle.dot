graph DoubleTriangle
{
	graph[center = true, margin = 0.2, nodesep = 0.1, ranksep = 0.3]
	node[shape = circle, fontsize = 8];
	edge[arrowsize = 0.6, fontsize = 8];

	0 [pos = "0, 0"];
	1 [pos = "2, 0"];
	2 [pos = "4, 0"];
	3 [pos = "1, 2"];
	4 [pos = "3, 2"];
	5 [pos = "2, 4"];

	0 -- 1 [label = "1.7"];
	0 -- 3 [label = "2.1"];
	1 -- 2 [label = "3.5"];
	1 -- 3 [label = "2.3"];
	1 -- 4 [label = "1.9"];
	2 -- 4 [label = "2.7"];
	3 -- 4 [label = "2.3"];
	3 -- 5 [label = "2.2"];
	4 -- 5 [label = "1.9"];
}

